<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GymMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GymMobile"
    Shell.FlyoutBehavior="Disabled"
    Title="GymMobile">

    <TabBar>
        <ShellContent
            Title="Fa muschi la falange"
            ContentTemplate="{DataTemplate local:MainPage}"
         />
        <ShellContent  
            Title="Alte sporturi"
            ContentTemplate="{DataTemplate local:DesprePage}" 
         />  
    </TabBar>

</Shell>
